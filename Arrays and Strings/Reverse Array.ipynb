   "source": [
    "def reverse(arr):\n",
    "    start,end = 0,len(arr)-1\n",
    "    while start < end:\n",
    "        arr[start],arr[end] = arr[end],arr[start]\n",
    "        start += 1\n",
    "        end -= 1\n",
    "    return arr"
   ]
