    "def biginteger_addition(arr1,arr2):\n",
    "    for i in range(len(arr1)):\n",
    "        arr1[i] = str(arr1[i])\n",
    "    for i in range(len(arr2)):\n",
    "        arr2[i] = str(arr2[i])\n",
    "    return int(''.join(arr1)) + int(''.join(arr2))    "
   ]
