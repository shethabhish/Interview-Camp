    "import math\n",
    "def max_diff(arr):\n",
    "    if len(arr) == 0:\n",
    "        return 0\n",
    "    minimum,maxdiff = math.inf,0\n",
    "    for i in range(len(arr)):\n",
    "        minimum = min(minimum,arr[i])\n",
    "        maxdiff = max(arr[i]-minimum,maxdiff)\n",
    "    return maxdiff"
   ]
  },
